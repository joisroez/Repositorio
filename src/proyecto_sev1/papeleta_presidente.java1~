    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton1MouseClicked
        try {
            // TODO add your handling code here:
            int vcorrea=1;
            if(jRadioButton1.isSelected()== true && jRadioButton2.isSelected()== false && jRadioButton3.isSelected()== false && jRadioButton4.isSelected()== false)
            {
                
                File entrada = new File("correa.txt");
                 FileWriter fw = new FileWriter(entrada,true);
                 PrintWriter pw = new PrintWriter(fw);
                 pw.println(vcorrea);
                pw.close();
            }











//
//            vnulos();
//            vblancos();
//            vcorrea();
//            vlucio();
//            vroldos();
////            vjamil();
//            System.out.println("b "+vblancos());
//             System.out.println("n "+vnulos());


            papeleta_diputados_alcaldes ir = new papeleta_diputados_alcaldes(); // crea objeto para ir a nueva ventana
            this.setVisible(false); //oculta la ventana actual
            ir.setVisible(true); // para que la ventana este siempre centrada
            ir.setLocationRelativeTo(null); // para que la ventana este siempre centrada
        } catch (FileNotFoundException ex) {
            Logger.getLogger(papeleta_presidente.class.getName()).log(Level.SEVERE, null, ex);
        } catch (IOException ex) {
            Logger.getLogger(papeleta_presidente.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_jButton1MouseClicked

