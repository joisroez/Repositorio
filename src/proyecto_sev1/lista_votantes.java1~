    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton1MouseClicked
        // TODO add your handling code here:
        String A="M1";
        String A1="F1";
        buscar busempa = new buscar("empa.txt");
        String datos = busempa.BuscarDatos(jTextField1.getText().toString());

        if (datos.equals("")){
            JOptionPane.showMessageDialog(null,"NO ESTA EMPADRONADO","ERROR",2);
            this.jPanel1.setVisible(false);
        } 
        else
        {
            this.jPanel1.setVisible(true);
            JOptionPane.showMessageDialog(null,"Registro encontrado","EXITO",2);
            StringTokenizer tok = new StringTokenizer(datos,"\t");
            tok.nextToken();
            String cedula = tok.nextToken().toString();
            String nombre = tok.nextToken().toString();
            String apellido = tok.nextToken().toString();
            String direccion = tok.nextToken().toString();
            String lugarnacimiento = tok.nextToken().toString();
            String estadocivil = tok.nextToken().toString();
            String sexo = tok.nextToken().toString();
            String dia = tok.nextToken().toString();
            String mes = tok.nextToken().toString();
            String anio = tok.nextToken().toString();
            String nacionalidad = tok.nextToken().toString();
            
            jTextField8.setText(cedula);
            jTextField2.setText( nombre);
            jTextField3.setText(apellido);
            jTextField4.setText(direccion);            
            jComboBox2.addItem(anio);
            jComboBox3.addItem(mes);
            jComboBox4.addItem(dia);
            jTextField6.setText(lugarnacimiento);
            jTextField7.setText(nacionalidad);
            jTextField9.setText(estadocivil);
            //asignamos el valor del empadronamiento en A y A1 y luego comparamos con el dato del fichero
            if(A1.equals(sexo))
            {
                F.setSelected(true);
            }
            if(A.equals(sexo))
            {
                M.setSelected(true);
            }
           
            //codigo sirve para cargar datos en los campos de fecha (EDITAR)
            for (int i = 1; i<=31; i++){
                jComboBox3.addItem(i + "");
            }
            for (int i = 1; i<=12; i++){
                jComboBox4.addItem(i + "");
            }
            for (int i = 1940; i<2013; i++){
                jComboBox2.addItem(i + "");
            }
        }
    }//GEN-LAST:event_jButton1MouseClicked

    private void jButton2MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton2MouseClicked
        // TODO add your handling code here:
                    editar ir = new editar();// crea objeto para ir a nueva ventana
                    this.setVisible(false);//oculta la ventana actual
                    ir.setVisible(true);//muestra la nueva ventana
                    ir.setLocationRelativeTo(null);// para que la ventana este siempre centrada
    }//GEN-LAST:event_jButton2MouseClicked

