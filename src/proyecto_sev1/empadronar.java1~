    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField1ActionPerformed
        // TODO add your handling code here:
    }//GEN-LAST:event_jTextField1ActionPerformed
 private void limpiarDatos(){
        jTextField1.setText("");
        jTextField2.setText("");
        jTextField3.setText("");
        GCodigo();
        jTextField7.setText("");
        jComboBox1.setSelectedIndex(0);
        jComboBox2.setSelectedIndex(0);
        M.setSelected(false);
        F.setSelected(false);
        jComboBox4.setSelectedIndex(0);
        jComboBox5.setSelectedIndex(0);
        jComboBox6.setSelectedIndex(0);
 }
  private boolean existeRegistro(){
        boolean existe = false;  
        buscar busca = new buscar("empa.txt");
        String dato = busca.BuscarDatos(jTextField3.getText().toString());
        if(dato.equals(""))
            existe = false;
        else
            existe = true;

        return existe;
    }
   private boolean listo(){
        boolean ok = false;
        
        if(jTextField1.getText().toString().equals("") || jTextField2.getText().toString().equals("") || jTextField3.getText().toString().equals("") || jTextField7.getText().toString().equals("") )
            ok = false;
        else
            ok = true;
        
        return ok;
    }
    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton1MouseClicked
        // TODO add your handling code here:
        String genero = "";
        if(M.isSelected()== true)
            genero = "M1";
        if(F.isSelected()==true)
            genero = "F1";
        persona empa= new persona(jTextField1.getText().toString(),jTextField2.getText().toString(),jTextField3.getText().toString(),jTextField4.getText(),jTextField7.getText().toString(),jTextField8.getText().toString(),jComboBox4.getSelectedItem().toString(),jComboBox5.getSelectedItem().toString(),jComboBox6.getSelectedItem().toString(),jComboBox1.getSelectedItem().toString(),jComboBox2.getSelectedItem().toString(), genero);
        //codigo para verificar si esta empadronado en (registro)
        String solo_cedula=jTextField3.getText();
        String datos = empa.datospersona();
        try{
            if(listo()==true){
                if(existeRegistro()==false){
                    File entrada = new File("empa.txt");
                    FileWriter fw = new FileWriter(entrada,true);
                    PrintWriter pw = new PrintWriter(fw);
                    pw.println(datos);
                    pw.close();

                    File entradacedula = new File("cedulas.txt");
                    FileWriter fwcedula = new FileWriter(entradacedula,true);
                    PrintWriter pcedula = new PrintWriter(fwcedula);
                    pcedula.println(solo_cedula);
                    pcedula.close();


                    JOptionPane.showMessageDialog(null,"Los datos han sido guardados","* * *   EXITO   * * *",2);
                    limpiarDatos();
                } else
                    JOptionPane.showMessageDialog(null,"USTED YA ESTA EMPADRONADO","* * *   ALTO   * * *",2);limpiarDatos();
            } else                JOptionPane.showMessageDialog(null,"Por favor llene todos los campos","* * *   ERROR   * * *",2);
        } catch(Exception e){
            JOptionPane.showMessageDialog(null,e.getMessage(),"* * *   ERROR   * * *",2);
        }
    }//GEN-LAST:event_jButton1MouseClicked

    private void jButton2MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton2MouseClicked
        // TODO add your handling code here:
        limpiarDatos();
    }//GEN-LAST:event_jButton2MouseClicked

    private void jButton3MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton3MouseClicked
        // TODO add your handling code here:
        editar ir = new  editar();// crea objeto para ir a nueva ventana
        this.setVisible(false);//oculta la ventana actual
        ir.setVisible(true);//muestra la nueva ventana
        ir.setLocationRelativeTo(null);// para que la ventana este siempre centrada
    }//GEN-LAST:event_jButton3MouseClicked

    private void jTextField3KeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jTextField3KeyPressed
        // TODO add your handling code here:
        String str=jTextField3.getText();
	boolean resultado=str.startsWith("-");
         if(resultado==true){
              JOptionPane.showMessageDialog(null, "Ingrese una cantidad valida");
              jTextField3.setText("");
         }

        if(!jTextField3.getText().equals(""))
        try{
            int  ci=Integer.parseInt(jTextField3.getText());
            }catch(NumberFormatException e){
             JOptionPane.showMessageDialog(null, "Solo se pueden ingresar numeros");
             jTextField3.setText("");
         }
    }//GEN-LAST:event_jTextField3KeyPressed

