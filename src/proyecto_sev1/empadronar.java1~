        Reloj modelo=new Reloj();
                modelo.addObserver (new Observer()
                 {
                     public void update (Observable unObservable, Object dato)
                     {
                         jLabel13.setText(""+dato);
                     }
                 });
    }

    private void GCodigo(){
        GenerarCodigo gcod = new GenerarCodigo("empa.txt");
        int cod = gcod.contarElementos();
        String salida = "";
        if (cod<10)
            salida = "0000" + cod;
        if(cod>=10 && cod<100)
            salida = "000" + cod;
        if(cod>=100 && cod<1000)
            salida = "00" + cod;
        if(cod>=1000 && cod<10000)
            salida = "0" + cod;
        if(cod>=10000 && cod<100000)
            salida = "" + cod;
        jTextField4.setText(salida);
    }

